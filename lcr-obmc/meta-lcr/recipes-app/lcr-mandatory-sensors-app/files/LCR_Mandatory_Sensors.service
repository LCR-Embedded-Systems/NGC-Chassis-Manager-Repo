[Unit]
Description=Mandatory Sensors Service
Requires=LCR_temp_sensors.service xyz.openbmc_project.fansensor.service
After=network.target LCR_temp_sensors.service xyz.openbmc_project.fansensor.service

[Service]
ExecStart=/usr/bin/mandatory_sensors
Restart=always
Type=simple

[Install]
WantedBy=multi-user.target